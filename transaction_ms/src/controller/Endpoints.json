{
    "name": "stocks",
    "moduleControllers": {
      "StockActions": {
        "path": "./controller/StocksAction.js",
        "urls": [
          {
            "url": "/transactions",
            "get": "getAllStockActions"
          },
          {
            "url": "/transactions/user",
            "get": "getStocksActionsByUser"
          },
          {
            "url": "/transactions/stock/:stock",
            "post": "buyOrSellStock"
          },
          {
            "url": "/stocks/symbols",
            "get": "getSymbolsData"
          }
        ]
      }
    }
}